  "jsnext:main": "diff-match-patch-ts.module.js",
  "types": "diff-match-patch-ts.module.d.ts",
  "scripts": {
    "build": "rm -rf dist && tsc -p tsconfig-esm.json && rollup -c rollup.config.js dist/diffMatchPatch.js > dist/diff-match-patch-ts.bundle.js && cp package.json dist && cp README.md dist && cp LICENSE.md dist",
    "test": "karma start --single-run",
    "npmpublish": "cd dist && npm publish",
    "commitmsg": "commitlint -e $GIT_PARAMS"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rars/diff-match-patch-ts"
  },
  "keywords": [
    "diff",
    "match",
    "patch"
